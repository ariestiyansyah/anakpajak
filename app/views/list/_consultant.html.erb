<div class="search-container">
    <% @consultants.each do |consultant| %>
            <div class="panel panel-default">
        <div class="panel-body">
            <div class="search-header">
                <a href="#" class="h4 inline-block"><%= consultant.title %></a>
                <div class="text-muted">Surat Ijin Praktek Â» <%= consultant.no_registration%></div>
            </div>
            <p class="m-top-sm">
                <%= consultant.address %>
            </p>
            <div class="text-right">
                <%= link_to("#payments",class: "navigation", data:{url:payments_path}) do %>
                    <span class="text">
                      Lapor Pajak
                    </span>
                    <span class="menu-hover"></span>
                <% end %>
            </div>
        </div>
    </div><!-- /panel -->
            <% end %>
</div><!-- /search-container -->
<script type="text/javascript">
    $("a.navigation").on("click",function () {
      var url = $(this).attr("data-url");
      $.ajax({                                      
        url   : url,
        type  : "GET"
      });
    });
    if (location.hash !=""){
      href = location.hash;
      $("a.navigation[href='"+href+"']").trigger("click");
    }
  </script>